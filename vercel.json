{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/board.js": {
      "runtime": "nodejs@18"
    },
    "api/tasks.js": {
      "runtime": "nodejs@18"
    },
    "api/team-members.js": {
      "runtime": "nodejs@18"
    },
    "api/categories.js": {
      "runtime": "nodejs@18"
    },
    "api/tasks/[id]/move.js": {
      "runtime": "nodejs@18"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
} 